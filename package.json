{
  "name": "chain-of-being",
  "version": "1.0.0",
  "description": "Layers of self-organizing emergent complexity",
  "engines": {
    "node": "11.6.0",
    "npm": "6.5.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf ./docs && parcel build --out-dir ./docs --public-url ./ ./src/index.html",
    "dev": "rm -rf ./dev && parcel --out-dir dev ./src/index.html",
    "tsc": "tsc --watch --noEmit",
    "prettier": "prettier --write './src/**/*.+(json|md|scss|ts|tsx)'",
    "tslint": "tslint --fix -p ./tsconfig.json './src/**/*.+(ts|tsx)'",
    "format": "npm run prettier && npm run tslint",
    "clean": "rm -rf .cache && rm -rf ./dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexburner/chain-of-being.git"
  },
  "keywords": [
    "complex",
    "integral",
    "systems",
    "emergence"
  ],
  "author": "aburner",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexburner/chain-of-being/issues"
  },
  "homepage": "https://github.com/alexburner/chain-of-being#readme",
  "devDependencies": {
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "parcel-bundler": "1.11.0",
    "prettier": "1.15.3",
    "sass": "1.15.2",
    "tslint": "5.12.0",
    "tslint-immutable": "5.0.1",
    "tslint-microsoft-contrib": "6.0.0",
    "typescript": "3.2.2"
  },
  "dependencies": {
    "react": "16.7.0",
    "react-dom": "16.7.0"
  }
}
